R"~(
program <- (var_def / func_def / class_def)* stmt* !. # !. makes sure the entire input is consumed!

class_def <- 'class' IDENT '(' IDENT ')' ':' NEWLINE 
	INDENT class_body DEDENT

class_body <- 'pass' NEWLINE
	/ (var_def / func_def)+

func_def <- 'def' IDENT '(' param_list? ')' return_type? ':' NEWLINE
	INDENT func_body DEDENT

param_list <- typed_var (',' typed_var)*

return_type <- '->' type

func_body <- (global_decl / nonlocal_decl / var_def / func_def)* stmt+

typed_var <- IDENT ':' type

type <- IDENT
	/ STRING # NOTE: Required to be a valid IDENT!
	/ '[' type ']'

global_decl <- 'global' IDENT NEWLINE

nonlocal_decl <- 'nonlocal' IDENT NEWLINE

var_def <- typed_var '=' literal NEWLINE



stmt <- if_stmt
	# / for_stmt
	/ while_stmt
	/ simple_stmt NEWLINE

simple_stmt <- 'pass'
	/ 'return' expr?
	/ (target '=')* expr

target <- IDENT (member_expr / index_expr)*

if_stmt <- 'if' expr ':' block
	('elif' expr ':' block)*
	('else' ':' block)?

while_stmt <- 'while' expr ':' block

# for_stmt <- 'for' IDENT 'in' expr ':' block

block <- NEWLINE INDENT stmt+ DEDENT



expr <- if_expr

if_expr <- or_expr ('if' or_expr 'else' if_expr)?

or_expr <- and_expr ('or' and_expr)*

and_expr <- not_expr ('and' not_expr)*

not_expr <- 'not' not_expr
	/ compare_expr

compare_expr <- add_expr (('==' / '!=' / '<=' / '>=' / '<' / '>' / 'is') add_expr)?

add_expr <- mul_expr (('+' / '-') mul_expr)*

mul_expr <- unary_expr (('*' / '//' / '/' / '%') unary_expr)* #NOTE: Added '/'

unary_expr <- '-' unary_expr
	/ '+' unary_expr #NOTE: Added unary '+'
	/ postfix_expr

postfix_expr <- primary (member_expr / index_expr / '(' arg_list? ')')*

member_expr <- '.' IDENT
index_expr <- '[' expr ']'
arg_list <- expr (',' expr)*

primary <- IDENT / literal / '(' expr ')' / list_literal

list_literal <- '[' (expr (',' expr)*)? ']'



# Lexical Rules

literal <- 'None'i
	/ 'True'i
	/ 'False'i
	/ FLOAT # Added floats
	/ INTEGER
	# / IDENTSTRING
	/ STRING

INTEGER <- [0-9]+
FLOAT <- < ([0-9]+'.'[0-9]* / '.'[0-9]+)([eE][+-]?[0-9]+)? >
STRING <- < SHORT_STRING / LONG_STRING >

SHORT_STRING <- SINGLE_QUOTED_STRING / DOUBLE_QUOTED_STRING
SINGLE_QUOTED_STRING <- "'" SHORT_SINGLE_CHAR* "'"
SHORT_SINGLE_CHAR <- ESCAPE_SEQUENCE / !("'" / "\\" / EOL) .
DOUBLE_QUOTED_STRING <- '"' SHORT_DOUBLE_CHAR* '"'
SHORT_DOUBLE_CHAR <- ESCAPE_SEQUENCE / !('"' / "\\" / EOL) .

LONG_STRING <- LONG_SINGLE_QUOTED_STRING / LONG_DOUBLE_QUOTED_STRING
LONG_SINGLE_QUOTED_STRING <- "'''" LONG_SINGLE_CHAR* "'''"
LONG_SINGLE_CHAR <- ESCAPE_SEQUENCE / !("'''") .
LONG_DOUBLE_QUOTED_STRING <- '"""' LONG_DOUBLE_CHAR* '"""'
LONG_DOUBLE_CHAR <- ESCAPE_SEQUENCE / !('"""') .

ESCAPE_SEQUENCE <- "\\" ESCAPE_CHAR
ESCAPE_CHAR <- [\\'"abfnrtv] / OCTAL_ESCAPE / HEX_ESCAPE / UNICODE_ESCAPE / EOL        # line continuation
OCTAL_ESCAPE <- [0-7]{1,3}
HEX_ESCAPE <- "x" [0-9a-f]i{2}
UNICODE_ESCAPE <- "u" [0-9a-f]i{4} / "U" [0-9a-f]i{8}


KEYWORD
  <- ('False' | 'None' | 'True' | 'and' | 'as' | 'assert' | 'async'
     | 'await' | 'break' | 'class' | 'continue' | 'def' | 'del' | 'elif'
     | 'else' | 'except' | 'finally' | 'for' | 'from' | 'global' | 'if'
     | 'import' | 'in' | 'is' | 'lambda' | 'nonlocal' | 'not' | 'or' | 'pass'
     | 'raise' | 'return' | 'try' | 'while' | 'with' | 'yield')
     !UIDENTContinue
IDENT <- !KEYWORD < UIDENTStart UIDENTContinue* >

